{
  "name": "ras-dashboard-full-stack",
  "version": "1.0.0",
  "description": "RAS Dashboard Full Stack Application",
  "scripts": {
    "dev": "npm run kill-ports && concurrently \"npm run dev:api\" \"npm run dev:client\"",
    "dev:unsafe": "concurrently \"npm run dev:api\" \"npm run dev:client\"",
    "dev:api": "cd api && npm run dev",
    "dev:client": "cd client && npm run dev",
    "dev:safe": "npm run kill-ports && concurrently \"npm run dev:api:safe\" \"npm run dev:client\"",
    "dev:api:safe": "cd api && npm run dev:safe:force",
    "start": "concurrently \"npm run start:api\" \"npm run start:client\"",
    "start:api": "cd api && npm start",
    "start:client": "cd client && npm run build && npm run preview",
    "install:all": "npm install && cd api && npm install && cd ../client && npm install",
    "clean": "rimraf api/node_modules client/node_modules node_modules",
    "kill-ports": "node scripts/kill-ports.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "dashboard",
    "api",
    "react",
    "full-stack"
  ],
  "author": "RAS Dashboard Team",
  "license": "MIT",
  "dependencies": {
    "@tanstack/react-query": "^5.83.0"
  }
}
